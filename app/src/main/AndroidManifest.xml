<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.staticmsi.meddroid">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NotificationActivity"
            android:label="@string/title_activity_notification" />
        <activity
            android:name=".AddPatientActivity"
            android:label="@string/title_activity_add_patient" />
        <activity
            android:name=".NeuroActivity"
            android:label="NeuroActivity"
            android:noHistory="true"></activity>
        <activity
            android:name=".RespActivity"
            android:label="RespActivity"
            android:noHistory="true" />
        <activity
            android:name=".CardioActivity"
            android:label="CardioActivity"
            android:noHistory="true" />
        <activity
            android:name=".GastroActivity"
            android:label="GastroActivity"
            android:noHistory="true" />
        <activity
            android:name=".GenitoryActivity"
            android:label="GenitoryActivity"
            android:noHistory="true" />
        <activity
            android:name=".PhysicalActivity"
            android:label="PhysicalActivity"
            android:noHistory="true" />
        <activity
            android:name=".ReportsActivity"
            android:label="@string/title_activity_reports" />
        <activity
            android:name=".ViewReportActivity"
            android:label="@string/title_activity_view_report" />
        <activity
            android:name=".NewEntryActivity"
            android:label="@string/title_activity_new_entry" />
        <activity
            android:name=".TestActivity"
            android:label="@string/title_activity_test">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddReportActivity"
            android:label="@string/title_activity_add_report" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login"
            android:noHistory="true"
            android:windowSoftInputMode="adjustResize|stateHidden"></activity>

        <!--<meta-data-->
            <!--android:name="com.google.android.gms.version"-->
            <!--android:value="@integer/google_play_services_version" />-->
    </application>

</manifest>
